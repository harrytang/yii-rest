{
    "name": "yiisoft/yii-rest",
    "description": "REST extension for the Yii framework",
    "keywords": ["yii", "rest", "api"],
    "type": "yii-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-rest/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-rest"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com",
            "homepage": "http://www.yiiframework.com/"
        },
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru",
            "homepage": "http://rmcreative.ru/"
        }
    ],
    "require": {
        "yiisoft/core": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {"yii\\rest\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"yiiunit\\rest\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
